		lout-to-modem_mode = <1>;
		spk-amplify = <2>;
		playback-if1-data_control = <0>;
		playback-if2-data_control = <0>;
	};

	tpm: tpm@20 {
		compatible = "infineon,slb9635tt";
		reg = <0x20>;
	};

};

